var accel, fric;
if (on_ground) {
    accel = S_RUN_ACCEL;
    fric = S_RUN_FRIC;
} else {
    accel = S_AIR_ACCEL;
    fric = S_AIR_FRIC;
}


if  (keyboard_check(vk_right)) {
    //Running right
    
    //First add friction if currently running right
    if (hspd &lt; 0)
        hspd = approach( hspd, 0, fric );
        
    hspd = approach( hspd, S_MAX_H, accel ); 
    sprite_index=spr_playerRunRight
    image_speed=.60
    facing=0
    
    
    }else if (keyboard_check_released( vk_right)){  
    //not running right
    sprite_index=spr_player_mask
    hspd=0  
    facing=0
    


} else if (keyboard_check( vk_left )){
    //Running left
    
    //First add friction if currently running left
    if (hspd &gt; 0)
        hspd = approach( hspd, 0, fric );
        
    hspd = approach( hspd, -S_MAX_H, accel ); 
    sprite_index=spr_playerRunLeft
    image_speed=.60
    facing=180

}else if (keyboard_check_released(vk_left)){
    //not running left
    sprite_index=spr_player_left
    hspd=0  
    facing=180

} else {
    //Stopping

    hspd = approach( hspd, 0, fric );
}


if (on_ground) {

    candj = true;
    
    //Jumping
    if (keyboard_check_pressed(vk_space))
        vspd = S_JUMP_SPEED;
              
}else {

    //Gravity
    vspd = approach( vspd, S_MAX_V, S_GRAVITY );

    //Double jumping
    if (keyboard_check_pressed( vk_space ) &amp;&amp; candj) {
        candj = false;
        vspd = S_DJUMP_SPEED;     
        
}
}
}


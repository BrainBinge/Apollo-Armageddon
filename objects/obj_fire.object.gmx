part_type_orientation(firepar,0,0,10,20,1)
part_type_gravity(firepar,0,270)
part_type_life(global.firepar,60,60)


//Creating the emitter so it looks like fire over a period
//No emitter will cause it to only to play once.
fire_emitter = part_emitter_create(fire)
part_emitter_region(fire,fire_emitter,x,x,y,y,ps_shape_ellipse,ps_distr_gaussian)
part_emitter_burst(fire,fire_emitter,firepar,5)


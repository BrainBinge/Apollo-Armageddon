fire = part_system_create();
part_system_depth(fire,10);

//Creating first Particle parts
firepar = part_type_create();
part_type_shape(firepar,pt_shape_flare)
part_type_size(firepar,.20,.10,0,0)
part_type_scale(firepar,.25,1.25)
part_type_colour3(firepar,c_red,c_orange,c_yellow)
